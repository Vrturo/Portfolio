	background: rgba(255, 255, 255, 0);
	-webkit-border-radius: 99px;
	-moz-border-radius: 99px;
	-ms-border-radius: 99px;
	-o-border-radius: 99px;
	border-radius: 99px;
	-webkit-transform: scale(1) translate3d(0, 0, 0);
	-moz-transform: scale(1) translate3d(0, 0, 0);
	-ms-transform: scale(1) translate3d(0, 0, 0);
	-o-transform: scale(1) translate3d(0, 0, 0);
	transform: scale(1) translate3d(0, 0, 0);
	-webkit-transition: all 400ms cubic-bezier(0.68, -0.55, 0.265, 1.55), background 200ms ease-out, opacity 200ms ease-in;
	-webkit-transition-delay: 0s, 0s, 100ms;
	-moz-transition: all 400ms cubic-bezier(0.68, -0.55, 0.265, 1.55), background 200ms ease-out, opacity 200ms ease-in 100ms;
	-o-transition: all 400ms cubic-bezier(0.68, -0.55, 0.265, 1.55), background 200ms ease-out, opacity 200ms ease-in 100ms;
	transition: all 400ms cubic-bezier(0.68, -0.55, 0.265, 1.55), background 200ms ease-out, opacity 200ms ease-in 100ms;
}

#cboxClose:hover,
  #cboxNext:hover,
  #cboxPrevious:hover {
	background: #ef4135;
}

#cboxClose:before,
  #cboxNext:before,
  #cboxPrevious:before {
	color: #fff;
}

#cboxTitle {
	font: 300 normal 20px/50px "Open Sans", Arial, sans-serif;
	color: #ccc;
}

#cboxCurrent {
	font: normal normal 16px/50px "Open Sans", Arial, sans-serif;
	color: #666;
}

#cboxLoadingGraphic {
	background: url(<%=asset_path "loading-large.gif"%>) center center no-repeat #0f1216;
	-webkit-border-radius: 40px;
	-moz-border-radius: 40px;
	-ms-border-radius: 40px;
	-o-border-radius: 40px;
	border-radius: 40px;
}

/* ----- single item ----- */
.gallery3d-item {
  /* item content holder */
  /* hover (icon) */
  /* hover (video icon) */
  /* hover (overlay & border) */
  /* disabled */
  /* active */
  /* placeholder */;
}

.gallery3d-item .gallery3d-item-content {
	background: #0F1216;
}

.gallery3d-item .gallery3d-item-hover {
	width: 36px;
	height: 36px;
	margin-left: -18px;
	margin-top: -18px;
	background: #ef4135;
	opacity: 0;
	-webkit-transform: scale(0);
	-moz-transform: scale(0);
	-ms-transform: scale(0);
	-o-transform: scale(0);
	transform: scale(0);
	-webkit-transition: all 200ms cubic-bezier(0.175, 0.885, 0.32, 1.275);
	-moz-transition: all 200ms cubic-bezier(0.175, 0.885, 0.32, 1.275);
	-o-transition: all 200ms cubic-bezier(0.175, 0.885, 0.32, 1.275);
	transition: all 200ms cubic-bezier(0.175, 0.885, 0.32, 1.275);
	-webkit-border-radius: 99px;
	-moz-border-radius: 99px;
	-ms-border-radius: 99px;
	-o-border-radius: 99px;
	border-radius: 99px;
}

.gallery3d-item .gallery3d-item-hover:before {
	content: "\e602";  /* plus icon */
	display: block;
	width: 16px;
	height: 16px;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-left: -8px;
	margin-top: -8px;
	font: normal normal 16px/16px gallery3d;
	color: #fff;
}

.gallery3d-item:hover .gallery3d-item-hover {
	opacity: 1;
	-webkit-transform: scale(1);
	-moz-transform: scale(1);
	-ms-transform: scale(1);
	-o-transform: scale(1);
	transform: scale(1);
}

.gallery3d-item[data-type="video"] .gallery3d-item-hover:before {
	content: "\e603";
	margin-left: -8px;
}

.gallery3d-item .gallery3d-item-content:before,
  .gallery3d-item .gallery3d-item-content:after {
	content: "";
	display: block;
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	left: 0;
	opacity: 0;
	outline: 1px solid transparent;
	background: #ef4135;
	-webkit-transition: all 200ms cubic-bezier(0.6, -0.28, 0.735, 0.045);
	-moz-transition: all 200ms cubic-bezier(0.6, -0.28, 0.735, 0.045);
	-o-transition: all 200ms cubic-bezier(0.6, -0.28, 0.735, 0.045);
	transition: all 200ms cubic-bezier(0.6, -0.28, 0.735, 0.045);
}

.gallery3d-item .gallery3d-item-content:after {
	z-index: -1;
	margin: -3px;
	padding: 3px;
	-webkit-transform: scale(0.9);
	-moz-transform: scale(0.9);
	-ms-transform: scale(0.9);
	-o-transform: scale(0.9);
	transform: scale(0.9);
}

.gallery3d-item:hover .gallery3d-item-content:before, .gallery3d-item.active .gallery3d-item-content:before, .gallery3d-item.active .gallery3d-item-content:after, .gallery3d-item.disabled.active .gallery3d-item-content:before, .gallery3d-item.disabled.active .gallery3d-item-content:after {
	display: block;
	opacity: 0.25;
	-webkit-transition: all 200ms cubic-bezier(0.175, 0.885, 0.32, 1.275);
	-moz-transition: all 200ms cubic-bezier(0.175, 0.885, 0.32, 1.275);
	-o-transition: all 200ms cubic-bezier(0.175, 0.885, 0.32, 1.275);
	transition: all 200ms cubic-bezier(0.175, 0.885, 0.32, 1.275);
}

.gallery3d-item.disabled .gallery3d-item-content:before,
  .gallery3d-item.disabled .gallery3d-item-content:after {
	display: none;
}

.gallery3d-item.active .gallery3d-item-content:after, .gallery3d-item.disabled.active .gallery3d-item-content:after {
	opacity: 1;
	-webkit-transform: scale(1);
	-moz-transform: scale(1);
	-ms-transform: scale(1);
	-o-transform: scale(1);
	transform: scale(1);
}

.gallery3d-item.active .gallery3d-item-hover, .gallery3d-item.disabled.active .gallery3d-item-hover {
	opacity: 0;
	-webkit-transform: scale(0);
	-moz-transform: scale(0);
	-ms-transform: scale(0);
	-o-transform: scale(0);
	transform: scale(0);
}

.gallery3d-item.gallery3d-item-placeholder .gallery3d-item-content:before,
  .gallery3d-item.gallery3d-item-placeholder .gallery3d-item-content:after, .gallery3d-item.gallery3d-item-placeholder:hover .gallery3d-item-content:before,
  .gallery3d-item.gallery3d-item-placeholder:hover .gallery3d-item-content:after {
	display: none;
}

.gallery3d-item.gallery3d-item-placeholder .gallery3d-item-hover, .gallery3d-item.gallery3d-item-placeholder:hover .gallery3d-item-hover {
	display: none;
}

/* ----- preview ----- */
.gallery3d-preview {
  /* preview content types */
  /* image preview */;
}

.gallery3d-preview .gallery3d-preview-image,
