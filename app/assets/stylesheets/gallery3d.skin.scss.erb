.gallery3d-preview .gallery3d-preview-image:before {
	content: "\e1b2";/* eye icon */
	display: block;
	width: 48px;
	height: 48px;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-left: -24px;
	margin-top: -24px;
