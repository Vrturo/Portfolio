/* ======================================================================= */
/* GALLERY3D SKIN
/* ======================================================================= */
#main-wrap {
	font: normal normal 13px/1.6em "Open Sans", Arial, sans-serif;
}

#main-wrap #main-overlay {
	background: rgba(255, 255, 255, 0.75);
}

/* ======================================================================= */
/* LOGO
/* ======================================================================= */
/* ----- logo ----- */
#logo p {
	font: 600 italic 11px/16px "Open Sans", Arial, sans-serif;
	color: #999;
}

/* ======================================================================= */
/* BASE
/* ======================================================================= */
h1, h2, h3, h4, h5, h6 {
	font-family: "Open Sans", Arial, sans-serif;
	font-weight: 300;
	color: #fff;
}

h1 small, h2 small, h3 small, h4 small, h5 small, h6 small {
	font-family: "Open Sans", Arial, sans-serif;
	font-weight: 600;
	text-transform: uppercase;
	word-spacing: 2px;
	color: #ef4135;
}

button, button[disabled], button[disabled]:hover {
	font: normal normal 13px/1.7em "Open Sans", Arial, sans-serif;
	text-transform: uppercase;
	color: #fff;
	border: 1px solid #0A0D11;
	background: #0A0D11;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	-ms-border-radius: 5px;
	-o-border-radius: 5px;
	border-radius: 5px;
	-webkit-transition: all 200ms ease-out;
	-moz-transition: all 200ms ease-out;
	-o-transition: all 200ms ease-out;
	transition: all 200ms ease-out;
}

button:hover {
	border-color: #ef4135;
	background: #ef4135;
}

button[type="reset"], button[type="reset"][disabled], button[type="reset"][disabled]:hover {
	color: #3D3E41;
	border: 1px solid #181C20;
	background: transparent;
}

button[type="reset"]:hover {
	color: #fff;
	border-color: #fff;
}

form label span {
	font: normal normal 12px/1.6em "Open Sans", Arial, sans-serif;
	text-transform: uppercase;
	color: #fff;
}

form input[type="text"],
form textarea {
	font: normal normal 13px/1.3em "Open Sans", Arial, sans-serif;
	color: #999;
	background: #0A0D11;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	-ms-border-radius: 5px;
	-o-border-radius: 5px;
	border-radius: 5px;
	-webkit-transition: color 200ms ease-out, background 200ms ease-out;
	-moz-transition: color 200ms ease-out, background 200ms ease-out;
	-o-transition: color 200ms ease-out, background 200ms ease-out;
	transition: color 200ms ease-out, background 200ms ease-out;
}

form input[type="text"]:focus,
  form textarea:focus {
	color: #fff;
	background: #05080c;
}

/* ======================================================================= */
/* ABOUT/CONTACT
/* ======================================================================= */
#about,
#contact {
	color: #999;
	background: #0f1216;
}

#about .nav-link,
  #contact .nav-link {
	font: normal normal 12px/40px "Open Sans", Arial, sans-serif;
	text-transform: uppercase;
	color: #999;
	background: #0F1216;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	-ms-border-radius: 5px;
	-o-border-radius: 5px;
	border-radius: 5px;
	-webkit-transition: background 200ms ease-out;
	-moz-transition: background 200ms ease-out;
	-o-transition: background 200ms ease-out;
	transition: background 200ms ease-out;
}

#about .nav-link:hover,
#contact .nav-link:hover {
	color: #fff;
	background: #ef4135;
}

#contact .contact-form-success,
#contact .contact-form-failure {
	padding-top: 90px;
}

#contact .contact-form-success:before,
  #contact .contact-form-failure:before {
	content: "\e030";
	display: block;
	width: 60px;
	height: 60px;
	position: absolute;
	top: 0;
	left: 50%;
	margin-left: -30px;
	font: normal normal 16px/60px gallery3d;
	color: #fff;
	background: #181C20;
	-webkit-border-radius: 99px;
	-moz-border-radius: 99px;
	-ms-border-radius: 99px;
	-o-border-radius: 99px;
	border-radius: 99px;
}

#contact .contact-form-failure:before {
	content: "\e146";
}

#contact form input,
#contact form textarea {
	border: 2px solid transparent;
}

#contact form input.invalid,
  #contact form textarea.invalid {
	border-color: #900;
}

#contact form button,
#contact form label {
	-webkit-transition: all 200ms ease-out;
	-moz-transition: all 200ms ease-out;
	-o-transition: all 200ms ease-out;
	transition: all 200ms ease-out;
	-webkit-transform: translate3d(0, 0, 0);
	-moz-transform: translate3d(0, 0, 0);
	-ms-transform: translate3d(0, 0, 0);
	-o-transform: translate3d(0, 0, 0);
	transform: translate3d(0, 0, 0);
}

#contact form.sending:before {
	content: "";
	display: block;
	width: 60px;
	height: 60px;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-top: -30px;
	margin-left: -30px;
	z-index: 9;
	background: url(<%=asset_path "loading-large.gif"%>) center center no-repeat #181c20;
	-webkit-border-radius: 99px;
	-moz-border-radius: 99px;
	-ms-border-radius: 99px;
	-o-border-radius: 99px;
	border-radius: 99px;
}

.copyright {
	font: 600 normal 12px/1.7em "Open Sans", Arial, sans-serif;
	color: #444;
}

.divider {
	border-color: #181C20;
}

/* ======================================================================= */
/* GALLERY3D
/* ======================================================================= */
/* ----- gallery3d ----- */
.gallery3d {
	top: -36px;
	padding-bottom: 36px;
	-webkit-transform: translate3d(0, 0, 0);
	-moz-transform: translate3d(0, 0, 0);
	-ms-transform: translate3d(0, 0, 0);
	-o-transform: translate3d(0, 0, 0);
	transform: translate3d(0, 0, 0);
}

/* ----- nav ----- */
.gallery3d-nav {
	bottom: 20px;
	-webkit-transform: translate3d(0, 0, 0);
	-moz-transform: translate3d(0, 0, 0);
	-ms-transform: translate3d(0, 0, 0);
	-o-transform: translate3d(0, 0, 0);
	transform: translate3d(0, 0, 0);
}

.gallery3d-nav .gallery3d-nav-link {
	font: 600 normal 11px/26px "Open Sans", Arial, sans-serif;
	text-transform: uppercase;
	color: #666;
	background: #e6e6e6;
	-webkit-border-radius: 99px;
	-moz-border-radius: 99px;
	-ms-border-radius: 99px;
	-o-border-radius: 99px;
	border-radius: 99px;
	-webkit-transition: all 200ms ease-out;
	-moz-transition: all 200ms ease-out;
	-o-transition: all 200ms ease-out;
	transition: all 200ms ease-out;
}

.gallery3d-nav .gallery3d-nav-link:hover {
	color: #333;
	background: #d6d6d6;
}

.gallery3d-nav .gallery3d-nav-link.active {
	color: #fff;
	background: #0F1216;
}

/* ----- pagination ----- */
.gallery3d-pagination-controls {
	-webkit-transition: opacity 200ms ease-out;
	-moz-transition: opacity 200ms ease-out;
	-o-transition: opacity 200ms ease-out;
	transition: opacity 200ms ease-out;
}

.gallery3d-pagination-controls li a {
	background: #C0C4C9;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	-ms-border-radius: 5px;
	-o-border-radius: 5px;
	border-radius: 5px;
	-webkit-transition: all 200ms ease-out;
	-moz-transition: all 200ms ease-out;
	-o-transition: all 200ms ease-out;
	transition: all 200ms ease-out;
}

.gallery3d-pagination-controls li a:hover {
	background: #3D3E3F;
}

.gallery3d-pagination-controls li.active a {
	background: #3D3E3F;
}

.gallery3d-pagination-next,
.gallery3d-pagination-prev {
	background: #3D3E3F;
	-webkit-border-radius: 99px;
	-moz-border-radius: 99px;
	-ms-border-radius: 99px;
	-o-border-radius: 99px;
	border-radius: 99px;
	-webkit-transform: scale(1) translate3d(0, 0, 0);
	-moz-transform: scale(1) translate3d(0, 0, 0);
	-ms-transform: scale(1) translate3d(0, 0, 0);
	-o-transform: scale(1) translate3d(0, 0, 0);
	transform: scale(1) translate3d(0, 0, 0);
	-webkit-transition: all 400ms cubic-bezier(0.68, -0.55, 0.265, 1.55), background 200ms ease-out, opacity 200ms ease-in;
	-webkit-transition-delay: 0s, 0s, 100ms;
	-moz-transition: all 400ms cubic-bezier(0.68, -0.55, 0.265, 1.55), background 200ms ease-out, opacity 200ms ease-in 100ms;
	-o-transition: all 400ms cubic-bezier(0.68, -0.55, 0.265, 1.55), background 200ms ease-out, opacity 200ms ease-in 100ms;
	transition: all 400ms cubic-bezier(0.68, -0.55, 0.265, 1.55), background 200ms ease-out, opacity 200ms ease-in 100ms;
}

.gallery3d-pagination-next:hover,
  .gallery3d-pagination-prev:hover {
	background: #ef4135;
}

.gallery3d-pagination-next:before,
  .gallery3d-pagination-prev:before {
	color: #fff;
}

.gallery3d.loading-items .gallery3d-pagination-next,
.gallery3d.loading-items .gallery3d-pagination-prev {
	-webkit-transform: scale(0) translate3d(0, 0, 0);
	-moz-transform: scale(0) translate3d(0, 0, 0);
	-ms-transform: scale(0) translate3d(0, 0, 0);
	-o-transform: scale(0) translate3d(0, 0, 0);
	transform: scale(0) translate3d(0, 0, 0);
	-webkit-transition: all 400ms cubic-bezier(0.68, -0.55, 0.265, 1.55), background 200ms ease-out, opacity 200ms ease-in;
	-moz-transition: all 400ms cubic-bezier(0.68, -0.55, 0.265, 1.55), background 200ms ease-out, opacity 200ms ease-in;
	-o-transition: all 400ms cubic-bezier(0.68, -0.55, 0.265, 1.55), background 200ms ease-out, opacity 200ms ease-in;
	transition: all 400ms cubic-bezier(0.68, -0.55, 0.265, 1.55), background 200ms ease-out, opacity 200ms ease-in;
}

.gallery3d-pagination-max .gallery3d-pagination-next,
.gallery3d-pagination-min .gallery3d-pagination-prev {
	background: #3D3E3F;
}

